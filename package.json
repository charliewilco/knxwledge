{
	"name": "@charliewilco/knxwledge",
	"version": "2.0.6",
	"license": "Unlicense",
	"type": "module",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"sideEffect": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./ds": {
			"types": "./dist/ds.d.ts",
			"import": "./dist/ds.js"
		},
		"./js": {
			"types": "./dist/js.d.ts",
			"import": "./dist/js.js"
		},
		"./problems": {
			"types": "./dist/problems.d.ts",
			"import": "./dist/problems.js"
		},
		"./search": {
			"types": "./dist/search.d.ts",
			"import": "./dist/search.js"
		},
		"./sorting": {
			"types": "./dist/sorting.d.ts",
			"import": "./dist/sorting.js"
		}
	},
	"scripts": {
		"build": "tsdown --config tsdown.config.ts",
		"test": "bun test",
		"coverage": "bun test --coverage",
		"lint": "biome lint ./src --log-level=error",
		"format": "biome format --write ./src *.json"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.15",
		"@types/bun": "latest",
		"lefthook": "^2.1.1",
		"publint": "^0.3.17",
		"tsdown": "^0.20.3",
		"typescript": "^5"
	},
	"publishConfig": {
		"@charliewilco:registry": "https://npm.pkg.github.com/"
	},
	"packageManager": "bun@1.3.6"
}
