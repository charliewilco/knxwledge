{
	"name": "@charliewilco/knxwledge",
	"version": "2.0.5",
	"license": "Unlicense",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"sideEffect": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"module": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./ds": {
			"types": "./dist/ds.d.ts",
			"module": "./dist/ds.js",
			"default": "./dist/ds.js"
		},
		"./js": {
			"types": "./dist/js.d.ts",
			"module": "./dist/js.js",
			"default": "./dist/js.js"
		},
		"./problems": {
			"types": "./dist/problems.d.ts",
			"module": "./dist/problems.js",
			"default": "./dist/problems.js"
		},
		"./search": {
			"types": "./dist/search.d.ts",
			"module": "./dist/search.js",
			"default": "./dist/search.js"
		},
		"./sorting": {
			"types": "./dist/sorting.d.ts",
			"module": "./dist/sorting.js",
			"default": "./dist/sorting.js"
		}
	},
	"scripts": {
		"build": "bun run scripts/build.ts",
		"clean": "bun run scripts/clean.ts",
		"test": "bun test",
		"coverage": "bun test --coverage",
		"lint": "biome lint ./src ./scripts --log-level=error",
		"format": "biome format --write ./src ./scripts *.json"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "latest",
		"dts-bundle-generator": "^9.5.1",
		"lefthook": "^1.11.13",
		"typescript": "^5"
	},
	"publishConfig": {
		"@charliewilco:registry": "https://npm.pkg.github.com/"
	}
}
